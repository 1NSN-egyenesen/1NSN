#!/bin/bash

wget -O /bin/radio --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/radio
chmod 777 /bin/radio

wget -O /bin/iso --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/iso
chmod 777 /bin/iso

wget -O /bin/rendszer --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/rendszer
chmod 777 /bin/rendszer

wget -O /bin/kernel --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/kernel
chmod 777 /bin/kernel

wget -O /bin/firmware --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/firmware
chmod 777 /bin/firmware

wget -O /bin/parancslista --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/parancslista
chmod 777 /bin/parancslista

wget -O /bin/xmt --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/xmt
chmod 777 /bin/xmt

wget -O /bin/epito --no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/epito
chmod 777 /bin/epito

wget -O /bin/fatdog https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/fatdog
chmod 777 /bin/fatdog

wget -O /bin/ventoy https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/ventoy
chmod 777 /bin/ventoy

wget -O /bin/wine https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/wine
chmod 777 /bin/wine

wget -O /bin/alap https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/alap
chmod 777 /bin/alap

echo "Scriptek letoltese kesz..."
echo "Ready..."
