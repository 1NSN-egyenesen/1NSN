#!/bin/bash
wget -O /bin/iso--no-check-certificate --content-disposition https://raw.githubusercontent.com/1NSN-egyenesen/1NSN/main/iso
chmod 777 /bin/iso

wget -O /bin/cache https://sourceforge.net/projects/egyenesenfatdog64/files/script/cache/download
chmod 777 /bin/cache

wget -O /bin/wine32 https://sourceforge.net/projects/egyenesenfatdog64/files/script/wine32/download
chmod 777 /bin/wine32

wget -O /bin/rendszer https://sourceforge.net/projects/egyenesenfatdog64/files/script/rendszer/download
chmod 777 /bin/rendszer

wget -O /bin/kernel https://sourceforge.net/projects/egyenesenfatdog64/files/script/kernel/download
chmod 777 /bin/kernel

wget -O /bin/firmware https://sourceforge.net/projects/egyenesenfatdog64/files/script/firmware/download
chmod 777 /bin/firmware

wget -O /bin/parancslista https://sourceforge.net/projects/egyenesenfatdog64/files/script/parancslista/download
chmod 777 /bin/parancslista

wget -O /bin/xmt https://sourceforge.net/projects/egyenesenfatdog64/files/script/xmt/download
chmod 777 /bin/xmt

wget -O /bin/epito https://sourceforge.net/projects/egyenesenfatdog64/files/script/epito/download
chmod 777 /bin/epito

wget -O /bin/tint https://sourceforge.net/projects/egyenesenfatdog64/files/script/tint/download
chmod 777 /bin/tint

echo "Scriptek letoltese kesz..."
echo "Ready..."
